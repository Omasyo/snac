<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group_1"
                android:pivotX="2"
                android:pivotY="16">
                <group
                    android:name="group"
                    android:pivotX="3"
                    android:pivotY="11">
                    <path
                        android:name="top"
                        android:pathData="M 3 5 L 3 11 L 21 11 L 21 5 Z M 4 11 L 8 5 M 8 11 L 12 5 M 12 11 L 16 5 M 16 11 L 20 5"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"
                        android:strokeLineJoin="round"/>
                </group>
                <path
                    android:name="bottom"
                    android:pathData="M 3 11 L 3 19 L 21 19 L 21 11 Z"
                    android:strokeColor="#000000"
                    android:strokeWidth="2"
                    android:strokeLineJoin="round"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="200"
                    android:duration="900"
                    android:valueFrom="-30"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/bounce_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="-30"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="200"
                    android:duration="700"
                    android:valueFrom="-30"
                    android:valueTo="15"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/anticipate_overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="900"
                    android:duration="200"
                    android:valueFrom="15"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="-1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="-1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="200"
                    android:valueFrom="0"
                    android:valueTo="-30"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="200"
                    android:duration="500"
                    android:valueFrom="-1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="200"
                    android:duration="500"
                    android:valueFrom="-1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
